  "authors": [
    "nullablepointer"
  ],
  "contact": {
    "issues": "https://github.com/nullablepointer/aspectratio/issues",
    "sources": "https://github.com/nullablepointer/aspectratio"
  },
  "license": "GPL-3.0-only",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.nullable.aspectratio.Aspectratio"
    ],
    "modmenu": [
      "me.nullable.aspectratio.ConfigScreen"
    ]
  },
  "mixins": [
    "aspectratio.mixins.json"
